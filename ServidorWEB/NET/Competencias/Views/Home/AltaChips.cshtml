@model Competencias.Models.CodigoChips

@{
    ViewData["Title"] = "Codigo Chips";
}

<h1>Alta</h1>

<h4>Chips & Dorsal</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AltaChips">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Codigo" class="control-label"></label>
                <input asp-for="Codigo" class="form-control" />
                <span asp-validation-for="Codigo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Dorsal" class="control-label"></label>
                <input asp-for="Dorsal" class="form-control" />
                <span asp-validation-for="Dorsal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}